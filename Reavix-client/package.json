{
  "name": "@reavix/client",
  "private": true,
  "version": "0.1.0",
  "description": "Official client SDK for Reavix backend framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.6.0",
    "vite-plugin-dts": "^4.5.4",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "reavix",
    "client",
    "sdk",
    "websocke",
    "http",
    "react"
  ]
}
